<!DOCTYPE html>
<head>
  <title>Project 2</title>
  <link rel="stylesheet" style="text/css" href="../../assets/stylesheets/style.css" >
</head>
<body>
  <div class="wrapper">
    <div class="header">
      <%= link_to(root_path) do %>
        <div class="logo">
        </div>
        <% end %>
      <div class="nav-social">
        <div class="social-icons">
          <ul>
            <li><a href=""><%= image_tag "facebook-icon.png" %></a></li>
            <li><a href=""><%= image_tag "twitter-icon.png" %></a></li>
            <li><a href=""><%= image_tag "instagram-icon.png" %><a/></li>
          </ul>
        </div>
        <div class="navigation">
          <ul>
            <%= link_to about_path do %>
              <li>About</li>
            <% end %>
            <%= link_to news_path do %>
              <li>News</li>
            <% end %>
            <%= link_to shows_path do %>
              <li>Shows</li>
            <% end %>
            <%= link_to music_path do %>
              <li>Music</li>
            <% end %>
            <%= link_to video_path do %>
              <li>Video</li>
            <% end %>
            <%= link_to photos_path do %>
              <li>Photos</li>
            <% end %>
          </ul>
        </div>
      </div>
    </div>
    <div class="container">
      <div class="content">
        <h1>Music</h1>
        <div class="music-list">
          <% @song.each do |song| %>
            <div class="track">
              <%= song.code.html_safe %>
            </div>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</body>
</html>